<keymap version="1" name="Default copy" parent="Default for XWin">
  <action id="Back">
    <mouse-shortcut keystroke="button4" />
    <keyboard-shortcut first-keystroke="shift ctrl alt p" />
  </action>
  <action id="Forward">
    <mouse-shortcut keystroke="button5" />
    <keyboard-shortcut first-keystroke="shift ctrl alt open_bracket" />
  </action>
  <action id="ReformatCode">
    <keyboard-shortcut first-keystroke="ctrl alt r" />
  </action>
  <action id="ShowSettings">
    <keyboard-shortcut first-keystroke="shift ctrl s" />
  </action>
  <action id="TogglePowerSave">
    <keyboard-shortcut first-keystroke="shift ctrl q" />
  </action>
  <action id="Vcs.QuickListPopupAction">
    <keyboard-shortcut first-keystroke="ctrl alt back_quote" />
  </action>
  <action id="Vcs.Show.Local.Changes">
    <keyboard-shortcut first-keystroke="alt g" />
  </action>
  <action id="VimPluginToggle">
    <keyboard-shortcut first-keystroke="shift alt v" />
  </action>
</keymap>